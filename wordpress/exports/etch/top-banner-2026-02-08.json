    "2602": {
      "id": 2602,
      "name": "Top Banner",
      "key": "TopBanner",
      "blocks": [
        {
          "blockName": "etch/element",
          "attrs": {
            "metadata": {
              "name": "Top Banner"
            },
            "script": {
              "code": "document.addEventListener('DOMContentLoaded', function () {\n  const banners = document.querySelectorAll('.announcement-bar');\n\n  banners.forEach((banner, index) => {\n    const bannerId = banner.id || `announcement-${index}`;\n    banner.id = bannerId;\n\n    const isDismissed = localStorage.getItem(`announcement-dismissed-${bannerId}`);\n    if (isDismissed) {\n      banner.style.display = 'none';\n      return;\n    }\n\n    const closeButton = banner.querySelector('.announcement__close');\n    if (!closeButton) return;\n\n    const dismiss = () => {\n      banner.style.display = 'none';\n      localStorage.setItem(`announcement-dismissed-${bannerId}`, 'true');\n    };\n\n    closeButton.addEventListener('click', function (event) {\n      event.preventDefault();\n      dismiss();\n    });\n\n    closeButton.addEventListener('keydown', function (event) {\n      if (event.key === 'Enter' || event.key === ' ') {\n        event.preventDefault();\n        dismiss();\n      }\n    });\n  });\n});",
              "id": "top-banner-dismiss"
            },
